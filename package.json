{
	"name": "@selemondev/svgl-svelte",
	"version": "2.0.6",
	"description": "An optimized package with SVG logos to be used as Svelte components âœ¨",
	"author": "Selemondev",
	"private": false,
	"license": "MIT",
	"packageManager": "pnpm@10.20.0",
	"scripts": {
		"dev": "vite dev",
		"build": "pnpm prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"generate:components": "pnpm dlx esno scripts/index.ts && pnpm format",
		"generate:release": "pnpm dlx changelogen@latest --release && git push --follow-tags",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write . && eslint . --fix",
		"lint:eslint": "eslint .",
		"lint:prettier": "prettier --check .",
		"lint": "pnpm lint:eslint && pnpm lint:prettier"
	},
	"files": [
		"dist",
		"!dist/components/*.d.ts",
		"!dist/types/*.d.ts",
		"!dist/utils/*.d.ts",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.0",
		"@sveltejs/adapter-auto": "^6.1.1",
		"@sveltejs/kit": "^2.48.4",
		"@sveltejs/package": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^5.1.1",
		"@types/node": "^22.18.13",
		"eslint": "^9.39.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"fs-extra": "^11.3.2",
		"globals": "^16.5.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"publint": "^0.3.15",
		"svelte": "^5.43.2",
		"svelte-check": "^4.3.3",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.2",
		"vite": "^6.4.1"
	},
	"keywords": [
		"svgl",
		"svgl-svelte",
		"logos",
		"svg",
		"svelte"
	],
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"publishConfig": {
		"access": "public"
	}
}
